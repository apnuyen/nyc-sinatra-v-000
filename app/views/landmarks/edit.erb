<h1>Edit Landmark</h1>

<form action="/landmarks/<%=@landmark.id%>" method="post">
  <input type="hidden" id="hidden" name="_method" value="patch">
  Name: <input type="text" id="name" name="landmark[name]" value="<%=@landmark.name%>">
  Year Completed: <input type="text" id="year_completed" name="landmark[year_completed]" value="<%=@landmark.year_completed.to_s%>">
<input type="submit" value="Edit Landmark">
</form>
